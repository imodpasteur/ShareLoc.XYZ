    "file-saver": "^2.0.5",
    "github-markdown-css": "^4.0.0",
    "highlight.js": "^10.1.1",
    "hypha-rpc": "^0.20.38",
    "json-stringify-safe": "^5.0.1",
    "jszip": "^3.6.0",
    "marked": "^4.0.10",
    "mathjs": "^7.0.1",
    "ol": "^6.3.1",
    "register-service-worker": "^1.7.1",
    "sortablejs": "^1.10.2",
    "spdx-license-list": "^6.4.0",
    "vue": "^2.6.11",
    "vue-form-json": "^3.5.2",
    "vue-js-modal": "github:imjoy-team/vue-js-modal#2.0.0-rc.3g",
    "vue-router": "^3.2.0",
    "vue-swatches": "^2.1.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-pwa": "~4.4.0",
    "@vue/cli-plugin-router": "~4.4.0",
    "@vue/cli-plugin-unit-mocha": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "chai": "^4.1.2",
    "copy-webpack-plugin": "^4.6.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "lodash": "^4.17.21",
    "prettier": "^1.19.1",
    "vue-cli-plugin-buefy": "~0.3.7",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-console": 0,
      "no-debugger": 0
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
